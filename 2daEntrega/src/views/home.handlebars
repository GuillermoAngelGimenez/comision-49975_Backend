<h1>Listado de Productos</h1>
<hr>

    {{#each productos}} 
        <div>
            <h4>{{this.title}}</h4>
            <p>Descripción: {{this.description}}</p>
            <p>Precio: {{this.price}} - Stock: {{this.stock}} </p>
            <hr>
        </div>
    {{/each}}

<hr>

